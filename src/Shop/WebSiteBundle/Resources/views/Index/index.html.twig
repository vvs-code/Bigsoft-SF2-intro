{% extends 'base.html.twig' %}
{% import 'WebSiteBundle:_common:macros.html.twig' as common %}

{% block title %}Main page{% endblock %}
{% block body %}
    {{ common.header() }}

    <ul class="items">
        {% for item in items %}
            <li>{{ common.printItem(item) }}</li>
        {% else %}
            <li>No items</li>
        {% endfor %}
    </ul>

    {{ common.footer() }}
{% endblock %}

